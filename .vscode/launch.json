    "version": "0.2.0",
    "configurations": [
        {
            "name": "CareClink",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define-from-file",
                ".env"
            ],
            "args": [
                "--target",
                "lib/main.dart",
                "--verbose"
            ]
        },
        {
            "name": "CareClink (profile mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define-from-file",
                ".env"
            ],
            "flutterMode": "profile",
            "args": [
                "--target",
                "lib/main.dart",
                "--verbose"
            ]
        },
        {
            "name": "CareClink (release mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define-from-file",
                ".env"
            ],
            "flutterMode": "release",
            "args": [
                "--target",
                "lib/main.dart",
                "--verbose"
            ]
        }
    ]
}
